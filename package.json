{
  "name": "js-sampler",
  "version": "0.0.4",
  "description": "Java Script Web Audio Sampler. Can read and play DecentSampler file format.",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/js-sampler.umd.js",
  "module": "./dist/js-sampler.es.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/js-sampler.es.js",
      "require": "./dist/js-sampler.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "sass": "^1.57.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.1",
    "vite-plugin-dts": "^1.7.2"
  },
  "dependencies": {
    "tone": "^14.7.77"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alfedukovich/js-sampler"
  },
  "bugs": {
    "url": "https://github.com/alfedukovich/js-sampler/issues"
  }
}
